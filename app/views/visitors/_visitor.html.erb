<div class="container-fluid">
<div id="<%= dom_id visitor %>" class="image_show">
  <div class="row">
    <div class="col-sm-6">
      <p>
        <strong>Name:</strong>
        <%= visitor.name %>
      </p>

      <p>
        <strong>Relation:</strong>
        <%= visitor.relation %>
      </p>

      <p>
        <strong>Classification:</strong>
        <%= visitor.classification %>
      </p>

      <p>
        <strong>No of visit:</strong>
        <%= visitor.no_of_visit %>
      </p>

      <p>
        <strong>Last visited:</strong>
        <%= visitor.last_visited %>
      </p>
    </div>
    <div class="col-sm-6">
      <div class="image_show">
        <p>
          <%= image_tag "data:image/jpeg;base64,#{visitor.image_string}", height: '496', width: '390' %>
        </p>
      </div>
    </div>
  </div>
</div>
  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4">
      <p>
        <strong>Notes:</strong>
        <%= visitor.notes %>
      </p>
   </div>
   <div class="col-sm-4"></div>
  </div>
</div>
