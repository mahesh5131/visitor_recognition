<div id="<%= dom_id visitor %>">
  <p>
    <strong>Name:</strong>
    <%= visitor.name %>
  </p>

  <p>
    <strong>Relation:</strong>
    <%= visitor.relation %>
  </p>

  <p>
    <strong>Classification:</strong>
    <%= visitor.classification %>
  </p>

  <p>
    <strong>No of visit:</strong>
    <%= visitor.no_of_visit %>
  </p>

  <p>
    <strong>Last visited:</strong>
    <%= visitor.last_visited %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= visitor.notes %>
  </p>

</div>
